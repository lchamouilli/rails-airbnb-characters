<div class="container_dash">
  <h1>DASHBOARD</h1>
  <div class="infos">
    <h2>Infos</h2>
  </div>

  <div class="row">

    <div class="col-12 col-md-6">
      <div class="pending">
        <h2>Mes pendings</h2>
        <% @mybookings.each do |character| %>

          <div class="card">
            <div class="card-image">
              <%= link_to character_path(character) do %>
                <% if character.photo.attached? %>
                  <%= cl_image_tag character.photo.key, width: 235, height:250 %>
                <%end%>
              <% end %>
            </div>
            <div class="card-infos">
              <ul class= "info-list">
                <li><h3><%=character.nickname%></h3></li>
                <li>
                  <% if character.average_rating == 5 %>
                    <i class="fa-solid fa-star"></i><i class="fa-solid fa-star"></i><i class="fa-solid fa-star"></i><i class="fa-solid fa-star"></i><i class="fa-solid fa-star"></i>
                  <% elsif character.average_rating == 4 %>
                    <i class="fa-solid fa-star"></i><i class="fa-solid fa-star"></i><i class="fa-solid fa-star"></i><i class="fa-solid fa-star"></i>
                  <% elsif character.average_rating == 3 %>
                    <i class="fa-solid fa-star"></i><i class="fa-solid fa-star"></i><i class="fa-solid fa-star"></i>
                  <% elsif character.average_rating == 2 %>
                    <i class="fa-solid fa-star"></i><i class="fa-solid fa-star"></i>
                  <% elsif character.average_rating == 1 %>
                    <i class="fa-solid fa-star"></i>
                  <% else %>
                    <i class="fa-solid fa-face-angry"></i>
                  <% end %>
                </li>
                <li>ðŸ”Ž <%=character.description%></li>
